<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.connectme">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity_hawkercentres"
            android:label="@string/title_activity_hawkercentres"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".activity_parkconnector"
            android:label="@string/title_activity_parkconnector"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity_settings"
            android:label="@string/title_activity_settings"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity_profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity_feedback"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".CreateAccountActivity"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MapsActivity"
            android:theme="@style/AppTheme.NoActionBar" />



        <activity android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SearchActivity"></activity>
        <meta-data android:name="com.google.android.geo.API_KEY" android:value="AIzaSyDSFviLWAsTyjKEwrYCEmH1fyTR7cIrrbo"/>




</application>

</manifest>